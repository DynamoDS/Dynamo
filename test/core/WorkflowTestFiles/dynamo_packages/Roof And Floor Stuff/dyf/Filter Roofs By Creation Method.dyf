    <Dynamo.Nodes.Python type="Dynamo.Nodes.Python" guid="33c18a20-cc98-4a66-a5f0-cfacba84305c" nickname="Python Script" x="592.66083916084" y="203.75" isVisible="true" isUpstreamVisible="true" lacing="Disabled">
      <Script>import clr
clr.AddReference('RevitAPI')
clr.AddReference('RevitAPIUI')
from Autodesk.Revit.DB import *
import Autodesk

roofinstances = IN
footprintlist = list()
extrusionlist = list()
baselist = list()
inplacelist = list()
noroofs = list()
for item in roofinstances:
	try:
		if item.GetType().Name == 'FootPrintRoof':
			footprintlist.append(item)
		elif item.GetType().Name == 'ExtrusionRoof':
			extrusionlist.append(item)
		elif item.GetType().Name == 'RoofBase':
			baselist.append(item)
		elif item.GetType().Name == 'FamilyInstance':
			inplacelist.append(item)
	except:
		noroofs.append(item)
OUT = (footprintlist,extrusionlist,baselist,inplacelist,noroofs)</Script>
