      <Script>import clr
clr.AddReference('RevitAPI')
clr.AddReference('RevitAPIUI')
from Autodesk.Revit.DB import *
import Autodesk

items = IN0
doc = IN1
elementlist = list()

for item in items:
	try:
		elementlist.append(doc.GetElement(item.Reference.ElementId))
	except:
		donothing = 1
OUT = elementlist</Script>
