    <Dynamo.Nodes.Python type="Dynamo.Nodes.Python" guid="aeac2166-5a09-4b62-bb08-627c234ee704" nickname="Python Script" x="172.857142857143" y="291.169642857143" isVisible="true" isUpstreamVisible="true" lacing="Disabled">
      <Script>#Copyright(c) 2014, Nathan Miller
# The Proving Ground, http://theprovingground.org
# Default imports
import clr
clr.AddReference('RevitAPI')
clr.AddReference('RevitAPIUI')
from Autodesk.Revit.DB import *
import Autodesk
import sys
import clr
path = r'C:\Autodesk\Dynamo\Core'
exec_path = r'C:\Autodesk\Dynamo\Core\dll'
sys.path.append(path)
sys.path.append(exec_path)
clr.AddReference('LibGNet')
from Autodesk.LibG import *

#The input to this node will be stored in the IN variable.

doc = __doc__

collector = FilteredElementCollector(doc)
collector.OfCategory(BuiltInCategory.OST_Rooms)
 
famtypeitr = collector.GetElementIdIterator()
famtypeitr.Reset()

output = []
rooms = []
names = []
numbers = []
areas = []
boundaries = []
locations = []
elementids = []
uniqueids = []
for item in famtypeitr:
    famtypeID = item
    faminst = doc.GetElement(famtypeID)
    
    #print faminst
    if faminst.Area &gt; 0:
    	room = faminst
    	roomname = faminst.Parameter['Name'].AsString()
    	number = faminst.Number
    	area = faminst.Area
    	location = faminst.Location.Point
    	elementid = faminst.Id.ToString()
    	uniqueid = faminst.UniqueId
    	
    	boptions = Autodesk.Revit.DB.SpatialElementBoundaryOptions()
    	boundsegs = room.GetBoundarySegments(boptions)
    	boundcurves = []
    	for bound in boundsegs:
    		crvs = []
    		for seg in bound:
    			crv = seg.Curve
    			crvs.append(crv)
    		boundcurves.append(crvs)
    	
    	rooms.append(room)
        names.append(roomname)
        numbers.append(number)
        areas.append(area)
        boundaries.append(boundcurves)
        locations.append(location)
        elementids.append(elementid)
        uniqueids.append(uniqueid)
        
output.append(rooms)
output.append(names)
output.append(numbers)
output.append(areas)
output.append(boundaries)
output.append(locations)
output.append(elementids)
output.append(uniqueids)
        
#Assign your output to the OUT variable
OUT = output</Script>
