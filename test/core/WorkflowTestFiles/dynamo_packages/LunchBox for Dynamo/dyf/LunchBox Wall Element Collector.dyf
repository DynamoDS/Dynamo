    <Dynamo.Nodes.Python type="Dynamo.Nodes.Python" guid="6a35c4d5-d9c1-49e4-8f9c-76f2d0d31fdc" nickname="Python Script" x="365.210475446091" y="588.877860860898" isVisible="true" isUpstreamVisible="true" lacing="Disabled">
      <Script>#Copyright(c) 2014, Nathan Miller
# The Proving Ground, http://theprovingground.org
# Default imports
import clr
clr.AddReference('RevitAPI')
clr.AddReference('RevitAPIUI')
from Autodesk.Revit.DB import *
import Autodesk
import sys
import clr
path = r'C:\Autodesk\Dynamo\Core'
exec_path = r'C:\Autodesk\Dynamo\Core\dll'
sys.path.append(path)
sys.path.append(exec_path)
clr.AddReference('LibGNet')
from Autodesk.LibG import *

#The input to this node will be stored in the IN variable.

doc = __doc__

walltype = IN

collector = FilteredElementCollector(doc)
collector.OfCategory(BuiltInCategory.OST_Walls)
collector.OfClass(Wall)
 
famtypeitr = collector.GetElementIdIterator()
famtypeitr.Reset()

output = []
walls = []
locations = []
heights = []
levelnames =[]
elementids = []
uniqueids = []
for item in famtypeitr:
    famtypeID = item
    faminst = doc.GetElement(famtypeID)
    
    if walltype == '*':
    	loc = faminst.Location.Curve
    	wall = faminst
    
    	levname = faminst.Parameter['Base Constraint'].AsValueString()
    	height = faminst.Parameter['Unconnected Height'].AsDouble()
    	elementid = faminst.Id.ToString()
    	uniqueid = faminst.UniqueId
    	
    	walls.append(wall)
    	locations.append(loc)
    	heights.append(height)
    	levelnames.append(levname)
    	elementids.append(elementid)
    	uniqueids.append(uniqueid)    
    elif faminst.WallType.Id == walltype.Id:
    	loc = faminst.Location.Curve
    	wall = faminst
    
    	levname = faminst.Parameter['Base Constraint'].AsValueString()
    	height = faminst.Parameter['Unconnected Height'].AsDouble()
    	elementid = faminst.Id.ToString()
    	uniqueid = faminst.UniqueId
    	
    	walls.append(wall)
    	locations.append(loc)
    	heights.append(height)
    	levelnames.append(levname)
    	elementids.append(elementid)
    	uniqueids.append(uniqueid)
        
output.append(walls)
output.append(locations)
output.append(heights)
output.append(levelnames)
output.append(elementids)
output.append(uniqueids)
        
#Assign your output to the OUT variable
OUT = output</Script>
