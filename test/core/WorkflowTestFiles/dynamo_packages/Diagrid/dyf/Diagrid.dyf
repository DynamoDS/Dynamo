import clr
clr.AddReference('RevitAPI')
clr.AddReference('RevitAPIUI')
from Autodesk.Revit.DB import *
import Autodesk
import sys
import clr
path = r'C:\Autodesk\Dynamo\Core'
exec_path = r'C:\Autodesk\Dynamo\Core\dll'
sys.path.append(path)
sys.path.append(exec_path)
clr.AddReference('LibGNet')
from Autodesk.LibG import *
import math
#The input to this node will be stored in the IN variable.
domain = IN[0]
uCount = IN[1]
vCount = IN[2]
face = IN[3]
rotated = IN[4]
nr = int(uCount*2)
nc = int(vCount)
outputList = []
vertexList = []
faceList = []


if uCount &gt; 0 and vCount &gt; 0:
	uUnit = float(domain.USpan/float(nc))
	vUnit = float(domain.VSpan/float(nr))

	# Create Vertices
	for aRow in range(0,int(nr + 1),1):
		if aRow%2 == 0:
			hoffset = 0.0
			deduct = int(0)
		else:
			hoffset = 0.5
			deduct = int(1)
		for aColumn in range(int(nc - deduct) + 1):
			aPoint = []
			u = float(aRow)*vUnit
			v = float(aColumn + hoffset)*uUnit
			# aPoint.append(u)
			# aPoint.append(v)
			if (rotated):
				vertexList.append(v)
				vertexList.append(u)
			else:
				vertexList.append(u)
				vertexList.append(v)

	# Create first set of faces
	for aRow in range(1,(nr + 1),2):
		for aColumn in range(1,(nc + 1),1):
			v1 = (aRow - 1)*(nc + 1) - ((aRow - 1)/2) + aColumn
			v2 = (aRow - 1)*(nc + 1) - ((aRow - 1)/2) + aColumn + 1
			v3 = (aRow - 1)*(nc + 1) - ((aRow - 1)/2) + aColumn + (nc + 1)
			faceList.append(v1 - 1)
			faceList.append(v2 - 1)
			faceList.append(v3 - 1)
			
			if (aColumn &lt; nc):
				v1 = (aRow - 1)*(nc + 1) - ((aRow - 1)/2) + aColumn + (nc + 1)
				v2 = (aRow - 1)*(nc + 1) - ((aRow - 1)/2) + aColumn + 1
				v3 = (aRow - 1)*(nc + 1) - ((aRow - 1)/2) + aColumn + (nc + 2)
				faceList.append(v1 - 1)
				faceList.append(v2 - 1)
				faceList.append(v3 - 1)

	# Create second set of faces
	for aRow in range(3,(nr + 2),2):
		for aColumn in range(1,(nc + 1),1):
			v1 = (aRow - 1)*(nc + 1) - ((aRow - 1)/2) + aColumn + 1
			v2 = (aRow - 1)*(nc + 1) - ((aRow - 1)/2) + aColumn
			v3 = (aRow - 3)*(nc + 1) - ((aRow - 3)/2) + aColumn + (nc + 1)
			faceList.append(v1 - 1)
			faceList.append(v2 - 1)
			faceList.append(v3 - 1)
			
			if (aColumn &lt; nc):
				v1 = (aRow - 3)*(nc + 1) - ((aRow - 3)/2) + aColumn + (nc + 1)
				v2 = (aRow - 3)*(nc + 1) - ((aRow - 3)/2) + aColumn + 1 + (nc + 1)
				v3 = (aRow - 1)*(nc + 1) - ((aRow - 1)/2) + aColumn + 1
				faceList.append(v1 - 1)
				faceList.append(v2 - 1)
				faceList.append(v3 - 1)

	# Create left and right triangular edges			
	for aRow in range(1,(nr + 1),2):
		v1 = (aRow - 1)*(nc + 1) - ((aRow - 1)/2) + 1
		v2 = (aRow - 1)*(nc + 1) - ((aRow - 1)/2) + 1 + (nc + 1)
		v3 = (aRow + 1)*(nc + 1) - ((aRow + 1)/2) + 1
		faceList.append(v1 - 1)
		faceList.append(v2 - 1)
		faceList.append(v3 - 1)
		
		v1 = (aRow + 1)*(nc + 1) - ((aRow + 1)/2) + nc + 1
		v2 = (aRow - 1)*(nc + 1) - ((aRow - 1)/2) + nc + (nc + 1)
		v3 = (aRow - 1)*(nc + 1) - ((aRow - 1)/2) + nc + 1
		faceList.append(v1 - 1)
		faceList.append(v2 - 1)
		faceList.append(v3 - 1)
	#Assign your output to the OUT variable
	outputList.append(vertexList)
	outputList.append(faceList)
	OUT = outputList
else:
	OUT = []
</Script>
