      <Script>import clr
clr.AddReference('RevitAPI')
clr.AddReference('RevitAPIUI')
from Autodesk.Revit.DB import *
import Autodesk

items = IN
elementlist = list()

for item in items:
	facelist = list()
	for face in item.Faces:
		facelist.append(face)
	elementlist.append(facelist)
OUT = elementlist</Script>
