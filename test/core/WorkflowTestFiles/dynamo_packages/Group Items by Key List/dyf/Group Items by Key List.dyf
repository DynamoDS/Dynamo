    <DSIronPythonNode.PythonNode type="DSIronPythonNode.PythonNode" guid="d465b3ad-d1a5-4bf2-882a-89ff84937a89" nickname="Python Script" x="234" y="59.2" isVisible="true" isUpstreamVisible="true" lacing="Disabled" inputcount="2">
      <Script>import clr
clr.AddReference('ProtoGeometry')
from Autodesk.DesignScript.Geometry import *
#The inputs to this node will be stored as a list in the IN variable.

# Sort data into bins according to a matching a set of criteria

# Get input
criteria = IN[0]	# List of criteria related to the data
data = IN[1]		# Data (list of same length as criteria) to be binned

# Find a unique set of values for the criteria, a list of all the bins
# to sort data into
bins = []
bins.append(criteria[0])
sorted = []
sorted.append([])

for i in range(1,len(criteria)):
	if criteria[i] in bins:
		continue
	else:
		bins.append(criteria[i])
		sorted.append([]) # initialize an empty list

# Sort data into bins
for i in range(len(criteria)):
	for j in range(len(bins)):
		if criteria[i] == bins[j]:
			temp = sorted[j]
			temp.append(data[i])
			sorted[j] = temp
			continue

# Assign your output to the OUT variable
OUT = sorted</Script>
