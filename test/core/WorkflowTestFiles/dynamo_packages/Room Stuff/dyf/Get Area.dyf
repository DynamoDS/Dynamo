      <Script>import clr
clr.AddReference('RevitAPI')
clr.AddReference('RevitAPIUI')
from Autodesk.Revit.DB import *
import Autodesk

faminsts = IN
elementlist = list()
for item in faminsts:
	try:
		elementlist.append(item.Area)
	except:
		elementlist.append(list())
OUT = elementlist</Script>
