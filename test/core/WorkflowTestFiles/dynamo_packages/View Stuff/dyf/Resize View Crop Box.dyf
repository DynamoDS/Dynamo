<Workspace Version="0.6.3.31052" X="-38.3375000000001" Y="250.905" zoom="0.8425" Description="Resizes the bounding box of a given Revit element by the specified amount in X, Y and Z direction. May need to be used in conjunction with a Transaction node." Category="CAAD_RWTH.Revit.Views" Name="Resize View Crop Box" ID="64a4fe31-b2f1-452a-8b9d-69dd81bc588d">
  <Elements>
    <Dynamo.Nodes.PythonVarIn type="Dynamo.Nodes.PythonVarIn" guid="7b9e5a34-a6cb-4019-8e83-842cbb3cba9e" nickname="Python Script With Variable Number of Inputs" x="413.96428571428" y="39.07142857143" isVisible="true" isUpstreamVisible="true" lacing="Disabled" inputs="2">
      <Script>import clr
clr.AddReference('RevitAPI')
clr.AddReference('RevitAPIUI')
from Autodesk.Revit.DB import *
import Autodesk

views = IN0
margin = IN1
elementlist = list()
failedlist = list()
for item in views:
	try:
		newmax = item.CropBox.Max.Add(XYZ(margin,margin,margin))
		newmin = item.CropBox.Min.Subtract(XYZ(margin,margin,margin))
		newbox = BoundingBoxXYZ()
		newbox.Max = newmax
		newbox.Min = newmin
		item.CropBox = newbox
		elementlist.append(item)
	except:
		do_nothing = 1
OUT = elementlist</Script>
