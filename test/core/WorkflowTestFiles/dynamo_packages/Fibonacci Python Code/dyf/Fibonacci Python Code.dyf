dataEnteringNode = IN


lst = []

def recursiveFib(v1,v2,count,lst):
	if count &lt; 1:
		return lst
	
	nextValue = v1 + v2
	lst.append(nextValue)

	return recursiveFib(v2, nextValue, count -1, lst)

def interativeFib(v1,v2, count):
	lst = []
	lst.append(v1)
	lst.append(v2)
	
	for i in range(0, count):
		nextValue = lst[-1] + lst[-2]
		
		lst.append(nextValue)
	return lst

