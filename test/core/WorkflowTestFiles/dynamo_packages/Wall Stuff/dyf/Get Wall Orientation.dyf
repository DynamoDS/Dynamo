      <Script>import clr
clr.AddReference('RevitAPI')
clr.AddReference('RevitAPIUI')
from Autodesk.Revit.DB import *
import Autodesk

wallinstances = IN
vectorlist = list()
for item in wallinstances:
	try:
		vectorlist.append(item.Orientation)
	except:
		vectorlist.append(list())
OUT = vectorlist</Script>
