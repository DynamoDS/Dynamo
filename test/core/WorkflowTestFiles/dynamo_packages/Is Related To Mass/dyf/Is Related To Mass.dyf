      <Script>import clr
clr.AddReference('RevitAPI')
clr.AddReference('RevitAPIUI')
from Autodesk.Revit.DB import *
import Autodesk

items = IN
booleans = list()
for item in items:
	try:
		booleans.append(item.get_Parameter(BuiltInParameter.RELATED_TO_MASS).AsInteger())
	except:
		booleans.append(0)
OUT = booleans</Script>
