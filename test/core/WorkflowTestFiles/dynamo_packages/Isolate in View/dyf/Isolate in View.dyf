    <Dynamo.Nodes.PythonVarIn type="Dynamo.Nodes.PythonVarIn" guid="d42f4774-d962-471a-8654-2b78ad49011a" nickname="Python Script With Variable Number of Inputs" x="644.883491002185" y="24.6264171894214" isVisible="true" isUpstreamVisible="true" lacing="Disabled" inputs="2">
      <Script># Default imports
import clr
clr.AddReference('RevitAPI')
clr.AddReference('RevitAPIUI')
from Autodesk.Revit.DB import *
import Autodesk
import sys
import clr
path = r'C:\Autodesk\Dynamo\Core'
exec_path = r'C:\Autodesk\Dynamo\Core\dll'
sys.path.append(path)
sys.path.append(exec_path)
clr.AddReference('LibGNet')
from Autodesk.LibG import *
from System.Collections.Generic import *
#The input to this node will be stored in the IN variable.
dataEnteringNode = IN0
view = IN0
str_ids = IN1
doc = __doc__

#get all elements in an active view
collector = FilteredElementCollector(doc, view.Id)

#create an element set from input string ids
element_set = ElementSet()
for i in str_ids:
	if isinstance(i, str):
		int_id = int(i)
		id = ElementId(int_id)
		element = doc.GetElement(id)
		element_set.Insert(element)
	else:
		element = doc.GetElement(i)
		element_set.Insert(element)
#create a set of element ids from element set
excludes = List[ElementId]()
elemIter = element_set.ForwardIterator()
elemIter.Reset()
while elemIter.MoveNext():
	curElem = elemIter.Current
	excludes.Add(curElem.Id)
#create exclusion filter containing input ids
filter = ExclusionFilter(excludes)
#create a collector that will select all elements in a view except one matching input element ids
new_collector = FilteredElementCollector(doc, view.Id).WhereElementIsNotElementType().WherePasses(filter).ToElementIds()
#hide elements in a view
view.HideElementsTemporary(new_collector)
#get active view name
view_name = view.ViewName
#Assign your output to the OUT variable
OUT = view_name</Script>
