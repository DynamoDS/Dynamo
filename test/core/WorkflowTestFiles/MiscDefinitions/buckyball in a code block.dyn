    <Dynamo.Nodes.CodeBlockNodeModel guid="93a30c71-6a3d-4cf3-8140-b5acc1d33cd6" type="Dynamo.Nodes.CodeBlockNodeModel" nickname="Code Block" x="-1498.21657154018" y="-171.792076650243" isVisible="false" isUpstreamVisible="true" lacing="Disabled" CodeText="// Phi&#xA;phi = (1 + Math.Sqrt(5))/2;&#xA;&#xA;// Scale factor&#xA;f = radius / Math.Sqrt(1 + 9*phi*phi);&#xA;&#xA;// Sets of coordinates&#xA;s1 = f * {{-1,1}*3*phi,     0,            {-1,1}   };&#xA;s2 = f * {{-1,1}*(1+2*phi), {-1,1}*phi,   {-1,1}*2 };&#xA;s3 = f * {{-1,1}*(2+phi),   {-1,1}*2*phi, {-1,1}   };&#xA;&#xA;// Points&#xA;p1 =  Point.ByCoordinates(s1[0]&lt;2&gt;, s1[1],    s1[2]&lt;1&gt;);&#xA;p2 =  Point.ByCoordinates(s2[0]&lt;2&gt;, s2[1]&lt;3&gt;, s2[2]&lt;1&gt;);&#xA;p3 =  Point.ByCoordinates(s3[0]&lt;2&gt;, s3[1]&lt;3&gt;, s3[2]&lt;1&gt;);&#xA;p3a = Point.ByCoordinates(s3[0]&lt;1&gt;, s3[1]&lt;2&gt;, s3[2]&lt;3&gt;);&#xA;&#xA;p4 =  Point.ByCoordinates(s1[2]&lt;1&gt;, s1[0]&lt;2&gt;, s1[1]   );&#xA;p5 =  Point.ByCoordinates(s2[2]&lt;1&gt;, s2[0]&lt;2&gt;, s2[1]&lt;3&gt;);&#xA;p6 =  Point.ByCoordinates(s3[2]&lt;1&gt;, s3[0]&lt;2&gt;, s3[1]&lt;3&gt;);&#xA;p6a = Point.ByCoordinates(s3[2]&lt;3&gt;, s3[0]&lt;1&gt;, s3[1]&lt;2&gt;);&#xA;&#xA;p7 =  Point.ByCoordinates(s1[1],    s1[2]&lt;1&gt;, s1[0]&lt;2&gt;);&#xA;p8 =  Point.ByCoordinates(s2[1]&lt;3&gt;, s2[2]&lt;1&gt;, s2[0]&lt;2&gt;);&#xA;p9 =  Point.ByCoordinates(s3[1]&lt;3&gt;, s3[2]&lt;1&gt;, s3[0]&lt;2&gt;);&#xA;p9a = Point.ByCoordinates(s3[1]&lt;2&gt;, s3[2]&lt;3&gt;, s3[0]&lt;1&gt;);&#xA;&#xA;// All vertices&#xA;Flatten({p1,p2,p3,p4,p5,p6,p7,p8,p9});&#xA;&#xA;// Edges&#xA;ax = Line.ByStartPointEndPoint(p1[0],p1[1]);&#xA;ay = Line.ByStartPointEndPoint(p4[0],p4[1]);&#xA;az = Line.ByStartPointEndPoint(p7[0],p7[1]);&#xA;&#xA;bx = Line.ByStartPointEndPoint(p3[0],p3[1]);&#xA;by = Line.ByStartPointEndPoint(p6[0],p6[1]);&#xA;bz = Line.ByStartPointEndPoint(p9[0],p9[1]);&#xA;&#xA;// These three lines don't work.  Instead, the next 12 must be used.&#xA;//cx = Line.ByStartPointEndPoint(p1,p2);&#xA;//cy = Line.ByStartPointEndPoint(p4,p5);&#xA;//cz = Line.ByStartPointEndPoint(p7,p8);&#xA;&#xA;cx1 = Line.ByStartPointEndPoint(p1[0][0],p2[0][0]);&#xA;cx2 = Line.ByStartPointEndPoint(p1[0][1],p2[0][1]);&#xA;cx3 = Line.ByStartPointEndPoint(p1[1][0],p2[1][0]);&#xA;cx4 = Line.ByStartPointEndPoint(p1[1][1],p2[1][1]);&#xA;&#xA;cy1 = Line.ByStartPointEndPoint(p4[0][0],p5[0][0]);&#xA;cy2 = Line.ByStartPointEndPoint(p4[0][1],p5[0][1]);&#xA;cy3 = Line.ByStartPointEndPoint(p4[1][0],p5[1][0]);&#xA;cy4 = Line.ByStartPointEndPoint(p4[1][1],p5[1][1]);&#xA;&#xA;cz1 = Line.ByStartPointEndPoint(p7[0][0],p8[0][0]);&#xA;cz2 = Line.ByStartPointEndPoint(p7[0][1],p8[0][1]);&#xA;cz3 = Line.ByStartPointEndPoint(p7[1][0],p8[1][0]);&#xA;cz4 = Line.ByStartPointEndPoint(p7[1][1],p8[1][1]);&#xA;&#xA;dx = Line.ByStartPointEndPoint(p2,p3);&#xA;dy = Line.ByStartPointEndPoint(p5,p6);&#xA;dz = Line.ByStartPointEndPoint(p8,p9);&#xA;&#xA;ex = Line.ByStartPointEndPoint(p2,p9a);&#xA;ey = Line.ByStartPointEndPoint(p5,p3a);&#xA;ez = Line.ByStartPointEndPoint(p8,p6a);&#xA;&#xA;// All edges&#xA;vertical = Flatten({ax,bx});&#xA;nonVert = Flatten({ay,az, by,bz, cx1,cx2,cx3,cx4, cy1,cy2,cy3,cy4, cz1,cz2,cz3,cz4, dx,dy,dz, ex,ey,ez});" ShouldFocus="false" />
