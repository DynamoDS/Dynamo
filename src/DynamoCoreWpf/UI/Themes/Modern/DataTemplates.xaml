<ResourceDictionary   xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                      xmlns:viewModels="clr-namespace:Dynamo.ViewModels"
                      xmlns:controls="clr-namespace:Dynamo.Controls"
                      xmlns:nodes="clr-namespace:Dynamo.Nodes"
                      xmlns:ui="clr-namespace:Dynamo.UI"
                      xmlns:sys="clr-namespace:System;assembly=mscorlib"
                      xmlns:models="clr-namespace:Dynamo.Models;assembly=DynamoCore">

    <DataTemplate x:Key="nodeViewModel"  DataType="{x:Type viewModels:NodeViewModel}">
        <controls:NodeView></controls:NodeView>
    </DataTemplate>

    <DataTemplate x:Key="noteViewModel" DataType="{x:Type viewModels:NoteViewModel}">
        <nodes:NoteView></nodes:NoteView>
    </DataTemplate>

    <DataTemplate x:Key="infoBubbleViewModel" DataType="{x:Type viewModels:InfoBubbleViewModel}">
        <controls:InfoBubbleView></controls:InfoBubbleView>
    </DataTemplate>

    <DataTemplate x:Key="annotationViewModel" DataType="{x:Type viewModels:AnnotationViewModel}">
        <nodes:AnnotationView></nodes:AnnotationView>
    </DataTemplate>

</ResourceDictionary>
