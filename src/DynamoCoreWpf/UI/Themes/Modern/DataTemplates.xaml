<ResourceDictionary   xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                      xmlns:viewModels="clr-namespace:Dynamo.ViewModels"
                      xmlns:controls="clr-namespace:Dynamo.Controls"
                      xmlns:nodes="clr-namespace:Dynamo.Nodes"
                      xmlns:ui="clr-namespace:Dynamo.UI"
                      xmlns:sys="clr-namespace:System;assembly=mscorlib"
                      xmlns:models="clr-namespace:Dynamo.Models;assembly=DynamoCore">

    <DataTemplate DataType="{x:Type viewModels:NodeViewModel}" x:Name="nodeViewModel">
        <controls:NodeView></controls:NodeView>
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewModels:NoteViewModel}" x:Name="noteViewModel">
        <nodes:NoteView></nodes:NoteView>
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewModels:InfoBubbleViewModel}" x:Name="infoBubbleViewModel">
        <controls:InfoBubbleView></controls:InfoBubbleView>
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewModels:AnnotationViewModel}" x:Name="annotationViewModel">
        <nodes:AnnotationView></nodes:AnnotationView>
    </DataTemplate>

</ResourceDictionary>
