    <!--  The default style for a TextBox in the Dark Theme  -->
    <Style x:Key="InputBoxDarkThemeStyle" TargetType="{x:Type TextBox}">
        <Setter Property="FontFamily" Value="{StaticResource ArtifaktElementRegular}" />
