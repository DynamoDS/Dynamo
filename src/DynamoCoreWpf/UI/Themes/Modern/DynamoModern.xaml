                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsMouseOver" Value="True">
                            <Setter TargetName="border" Property="Opacity" Value="0.9" />
                            <Setter TargetName="mouseOverBorder" Property="Visibility" Value="Visible" />
