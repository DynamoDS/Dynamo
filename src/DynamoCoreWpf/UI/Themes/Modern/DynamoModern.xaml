                                <Image.Width>
                                    <Binding Path="Width" RelativeSource="{RelativeSource TemplatedParent}" />
                                </Image.Width>
                                <Image.Source>
                                    <Binding Path="StateImage" RelativeSource="{RelativeSource TemplatedParent}" />
                                </Image.Source>
                                <Image.RenderTransform>
                                    <TranslateTransform />
                                </Image.RenderTransform>
                            </Image>
                            <Image Name="buttonCheckImage">
                                <Image.Width>
