                            <Setter TargetName="text" Property="Margin" Value="0,3,1,0" />
                            <Setter TargetName="container" Property="Margin" Value="1,0,0,0" />
                            <Setter TargetName="roundedBorder" Property="CornerRadius" Value="0,4,4,0" />
