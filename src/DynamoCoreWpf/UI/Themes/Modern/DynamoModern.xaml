                    <Grid>
                        <Border x:Name="mouseOverBorder"
                                Margin="2"
                                BorderBrush="#ECECEC"
                                BorderThickness="2"
                                CornerRadius="2"
                                Visibility="Hidden" />
                        <Border x:Name="mousePressedBorder"
                                Margin="0"
                                BorderBrush="{StaticResource PrimaryCharcoal200Brush}"
                                BorderThickness="4"
                                CornerRadius="4"
                                Visibility="Hidden" />
