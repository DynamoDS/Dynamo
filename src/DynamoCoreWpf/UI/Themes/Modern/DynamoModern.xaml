                <ControlTemplate>

                    <Border x:Name="container"
                            Background="Transparent"
                            BorderBrush="#3F3F3F"
                            BorderThickness="2">
                        <Grid x:Name="inner" Background="#CCCCCC">

                            <TextBlock x:Name="text"
                                       Margin="10,10,10,10"
                                       HorizontalAlignment="Center"
                                       VerticalAlignment="Center"
