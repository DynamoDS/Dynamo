  <ItemGroup>
    <Page Include="Controls\CustomPropertyControl.xaml">
      <SubType>Designer</SubType>
      <Generator>MSBuild:Compile</Generator>
    </Page>
    <Page Include="Controls\ImageSelector.xaml">
      <Generator>MSBuild:Compile</Generator>
      <SubType>Designer</SubType>
    </Page>
    <Page Include="GraphMetadataView.xaml">
      <SubType>Designer</SubType>
      <Generator>MSBuild:Compile</Generator>
