<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <UILib>true</UILib>
    </PropertyGroup>
  <ImportGroup Label="PropertySheets">
    <Import Project="$(SolutionDir)Config\CS_SDK.props" />
  </ImportGroup>
  <PropertyGroup>
