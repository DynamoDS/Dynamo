<Window x:Class="Dynamo.Applications.DynamoVersionSelector"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             Height="240" Width="300">
    <Grid>
        <StackPanel Margin="5" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Height="Auto">
            <Image Source="/DynamoRevitVersionSelector;component/logo_about.png" Height="128" Width="128"></Image>
            <TextBlock HorizontalAlignment="Center" Margin="5" Text="Choose a version of Dynamo."></TextBlock>
            <ComboBox Name="VersionDropDown" Margin="5" ItemsSource="{Binding Versions}"></ComboBox>
        </StackPanel>
    </Grid>
</Window>
