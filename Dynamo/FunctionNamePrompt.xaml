<Window x:Class="Dynamo.Elements.FunctionNamePrompt"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="New Node" Height="197" Width="238" ResizeMode="NoResize" WindowStartupLocation="CenterOwner">
    <Grid Height="Auto">
      <Label Content="Name:" Height="28" HorizontalAlignment="Stretch" Margin="11,1,12,0" Name="label1" VerticalAlignment="Top" />
      <TextBox Height="23" HorizontalAlignment="Stretch" Margin="12,29,12,0" Name="nameBox" VerticalAlignment="Top" Width="192" TabIndex="0" />
      <Button Content="Cancel" Height="23" HorizontalAlignment="Right" Margin="0,123,13,0" Name="cancelButton" VerticalAlignment="Top" Width="75" Click="Cancel_Click" IsCancel="True" TabIndex="3" />
      <Button Content="OK" Height="23" HorizontalAlignment="Right" Margin="0,123,94,0" Name="okButton" VerticalAlignment="Top" Width="75" Click="OK_Click" IsDefault="True" TabIndex="2" />
      <ComboBox Height="23" HorizontalAlignment="Stretch" Margin="12,87,12,0" Name="categoryBox" VerticalAlignment="Top" Width="192" IsEditable="True" TabIndex="1" />
      <Label Content="Category:" Height="28" HorizontalAlignment="Stretch" Margin="12,59,13,0" Name="label2" VerticalAlignment="Top" />
   </Grid>
</Window>
